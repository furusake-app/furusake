{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "main": "./src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "dply": "wrangler deploy --minify dist/index.js",
    "predply": "hono optimize src/index.ts -m",
    "check": "biome check .",
    "fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "gen:cf-type": "wrangler types --env-interface CloudflareBindings"
  },
  "dependencies": {
    "hono": "^4.9.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "wrangler": "^4.4.0"
  }
}
