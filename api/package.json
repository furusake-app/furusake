{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./types/index.d.ts",
  "scripts": {
    "dev": "wrangler dev",
    "dply": "wrangler deploy --minify dist/index.js",
    "predply": "pnpm build:hono",
    "check": "biome check .",
    "fix": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "gen:cf-type": "wrangler types --env-interface CloudflareBindings",
    "build:hono": "hono optimize src/index.ts -o dist/index.js -m",
    "build:type": "tsdown"
  },
  "dependencies": {
    "hono": "^4.9.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "tsdown": "^0.15.9",
    "wrangler": "^4.4.0"
  }
}
